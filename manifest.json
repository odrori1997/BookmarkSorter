{
	"manifest_version": 2,
	"name": "Link Sort",
	"version": "0.1",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_scripts": [
	{
		"matches": [
			"<all_urls>"
		],
		"js": ["content.js"]
	}
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	}
}